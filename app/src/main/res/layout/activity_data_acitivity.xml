<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#07f9f1"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView9"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/textView9"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="15dp"
        android:text="Latency"
        android:textColor="#0e0e0e"
        android:textSize="20dp"
        android:textStyle="bold" />

    <com.hookedonplay.decoviewlib.DecoView
        android:id="@+id/data_latency"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_above="@+id/textView2"
        android:layout_centerHorizontal="true"
        app:dv_lineWidth="20dp" />

    <TextView
        android:id="@+id/data_pecentL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/data_latency"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="41dp"
        android:text="80%"
        android:textSize="25dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/data_levelL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/data_pecentL"
        android:layout_centerHorizontal="true"
        android:text="Average"
        android:textColor="#ff00"
        android:textSize="16dp" />

    <TextView
        android:textStyle="bold"
        android:textColor="#0e0e0e"
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/data_ipdv"
        android:layout_alignEnd="@+id/textView9"
        android:layout_alignRight="@+id/textView9"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="IPDV"
        android:textSize="20dp" />

    <com.hookedonplay.decoviewlib.DecoView
        android:id="@+id/data_ipdv"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_above="@+id/textView9"
        android:layout_alignLeft="@+id/data_latency"
        android:layout_alignStart="@+id/data_latency"
        app:dv_lineWidth="20dp" />

    <TextView
        android:id="@+id/data_pecentipdv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/data_pecentL"
        android:layout_alignStart="@+id/data_pecentL"
        android:layout_centerVertical="true"
        android:text="80%"
        android:textSize="25dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/data_levelipdv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/data_levelL"
        android:layout_alignStart="@+id/data_levelL"
        android:layout_below="@+id/data_pecentipdv"
        android:text="Average"
        android:textColor="#ff00"
        android:textSize="16dp" />
    <TextView
        android:textStyle="bold"
        android:textColor="#0e0e0e"
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/data_losspacket"
        android:layout_alignEnd="@+id/data_ipdv"
        android:layout_alignRight="@+id/data_ipdv"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:text="Loss packet"
        android:textSize="20dp" />

    <com.hookedonplay.decoviewlib.DecoView
        android:id="@+id/data_losspacket"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignLeft="@+id/data_ipdv"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/data_ipdv"
        android:layout_marginBottom="15dp"
        app:dv_lineWidth="20dp" />

    <TextView
        android:id="@+id/data_pecentloss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView2"
        android:layout_alignRight="@+id/textView2"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="39dp"
        android:text="80%"
        android:textSize="25dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/data_levelloss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/data_pecentipdv"
        android:layout_alignStart="@+id/data_pecentipdv"
        android:layout_below="@+id/data_pecentloss"
        android:text="Average"
        android:textColor="#ff00"
        android:textSize="16dp" />
</RelativeLayout>
